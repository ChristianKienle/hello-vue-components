{
  "name": "hello-vue-components",
  "description": "A simple, example component library using Vue CLI 3.",
  "version": "1.1.1",
  "author": {
    "name": "Chris Fritz",
    "email": "chrisvfritz@gmail.com"
  },
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/hello-vue-components.common.js",
  "unpkg": "dist/hello-vue-components.umd.min.js",
  "jsdelivr": "dist/hello-vue-components.umd.min.js",
  "homepage": "https://github.com/chrisvfritz/hello-vue-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisvfritz/hello-vue-components.git"
  },
  "bugs": {
    "url": "https://github.com/chrisvfritz/hello-vue-components/issues"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node bin/build-lib.js",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "peerDependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
