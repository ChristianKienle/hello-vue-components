{
  "name": "hello-vue-components",
  "description": "A simple, example component library using Vue CLI 3.",
  "version": "1.2.2",
  "author": {
    "name": "Chris Fritz",
    "email": "chrisvfritz@gmail.com"
  },
  "license": "MIT",
  "files": ["src", "dist", "LICENSE", "README.md"],
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.min.js",
  "jsdelivr": "dist/index.umd.min.js",
  "homepage": "https://github.com/chrisvfritz/hello-vue-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisvfritz/hello-vue-components.git"
  },
  "bugs": {
    "url": "https://github.com/chrisvfritz/hello-vue-components/issues"
  },
  "scripts": {
    "dev": "vue-cli-service serve demo/main.js",
    "build": "node utils/build-lib.js",
    "lint":
      "eslint --ext .js,.vue --fix . && stylelint --fix \"{src,demo}/**/*.{css,vue}\" && markdownlint docs/*.md *.md && prettier --write --loglevel warn \"**/*.{js,json,css,vue,md}\"",
    "unit": "vue-cli-service test:unit",
    "e2e": "npm run build && vue-cli-service test:e2e --headless",
    "test": "npm run unit && npm run e2e",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "peerDependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@vue/cli-plugin-babel": "^3.1.0",
    "@vue/cli-plugin-e2e-cypress": "^3.1.0",
    "@vue/cli-plugin-eslint": "^3.1.0",
    "@vue/cli-plugin-unit-jest": "^3.1.0",
    "@vue/cli-service": "^3.1.0",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.25",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "eslint-plugin-vue": "5.0.0-beta.3",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^8.0.4",
    "lodash": "^4.17.11",
    "markdownlint-cli": "^0.13.0",
    "ncp": "^2.0.0",
    "stylelint": "^9.7.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "val-loader": "^1.1.1",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"]
}
